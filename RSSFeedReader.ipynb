   "source": [
    "import requests\n",
    "from bs4 import BeautifulSoup"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "7cf658da",
   "metadata": {},
   "outputs": [],
   "source": [
    "def generate_feed(url):\n",
    "    response = requests.get(url)\n",
    "    response.raise_for_status()\n",
    "    soup = BeautifulSoup(response.content, 'xml')\n",
    "    items = soup.findAll('item')\n",
    "    for item in items:\n",
    "        title = item.title.text\n",
    "        description = item.description.text\n",
    "        link = item.link.text\n",
    "        print(f'Title: {title}')\n",
    "        print(f'Description: {description}')\n",
    "        print(f'Link: {link}')\n",
    "        print()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "e3ee273d",
   "metadata": {},
   "outputs": [],
   "source": [
    "def main():\n",
    "    while True:\n",
    "        try:\n",
    "            url = input('> ')\n",
    "            generate_feed(url)\n",
    "        except Exception:\n",
    "            break"
   ]
  },
